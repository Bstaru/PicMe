<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>IniciarSesion</servlet-name>
        <servlet-class>Servlet.IniciarSesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrarUsuarios</servlet-name>
        <servlet-class>Servlet.RegistrarUsuarios</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LeerImagen</servlet-name>
        <servlet-class>Servlet.LeerImagen</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LeerImagenCov</servlet-name>
        <servlet-class>Servlet.LeerImagenCov</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditarUsuario</servlet-name>
        <servlet-class>Servlet.EditarUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditarPerfil</servlet-name>
        <servlet-class>Servlet.EditarPerfil</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LeerImagenPost</servlet-name>
        <servlet-class>Servlet.LeerImagenPost</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrarPublicacion</servlet-name>
        <servlet-class>Servlet.RegistrarPublicacion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BorrarPublicacion</servlet-name>
        <servlet-class>Servlet.BorrarPublicacion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditarPost</servlet-name>
        <servlet-class>Servlet.EditarPost</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>buscarWeas</servlet-name>
        <servlet-class>Servlet.buscarWeas</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>IniciarSesion</servlet-name>
        <url-pattern>/iniciar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrarUsuarios</servlet-name>
        <url-pattern>/nuevousuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LeerImagen</servlet-name>
        <url-pattern>/LeerImagen</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LeerImagenCov</servlet-name>
        <url-pattern>/LeerImagenCov</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditarUsuario</servlet-name>
        <url-pattern>/editar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditarPerfil</servlet-name>
        <url-pattern>/editarPerfil</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LeerImagenPost</servlet-name>
        <url-pattern>/imagenPost</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrarPublicacion</servlet-name>
        <url-pattern>/nuevoPost</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BorrarPublicacion</servlet-name>
        <url-pattern>/BorrarPublicacion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditarPost</servlet-name>
        <url-pattern>/EditarPost</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>buscarWeas</servlet-name>
        <url-pattern>/buscarWeas</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
